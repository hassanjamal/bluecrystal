{
    "require": {
            "laravel/framework": "4.1.*",
            "cartalyst/sentry": "2.0.*",
            "j20/php-uuid": "dev-master",
            "bllim/datatables": "*",
            "dflydev/markdown": "1.0.*@dev",
            "laravelbook/ardent": "dev-master",
            "barryvdh/laravel-dompdf": "0.3.1"
    },
        "require-dev": {
            "way/generators": "2.*",
            "itsgoingd/clockwork": "1.*"
        },
        "autoload": {
            "classmap": [
            "app/commands",
            "app/controllers",
